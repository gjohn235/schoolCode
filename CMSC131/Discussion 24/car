public class Car
{
  private String make;
  private int year;
  private int value;

  public Car(String make, int year, int value)
  {
    this.make = make;
    this.year = year;
    this.value = value;
  }
  public String getMake()
  {
    return make;
  }
  public int getYear()
  {
    return year;
  }
  public int getValue()
  {
    return value;
  }
  public String toString()
  {
    String correctFormat;
    correctFormat = make + " " + year + " $" + value;
    return correctFormat;
  }
}
